{
  "name": "myparcelcom/integration-commons",
  "description": "Library with common classes used for constructing MyParcel.com eCommerce Integrations",
  "type": "library",
  "require": {
    "php": ">=8.2",
    "laravel/framework": ">=8.40",
    "ramsey/uuid": "^4.1"
  },
  "require-dev": {
    "fakerphp/faker": "^1.13",
    "mockery/mockery": "^1.4",
    "phpunit/phpunit": ">=9.5",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "MyParcelCom\\Integration\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "MyParcel.com",
      "email": "info@myparcel.com"
    }
  ],
  "minimum-stability": "stable"
}
